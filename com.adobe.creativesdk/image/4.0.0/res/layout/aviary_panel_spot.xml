<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="?attr/aviaryBottomBarOptionPanelContainerStyleNoMargin"
	android:orientation="vertical">

	<com.aviary.android.feather.sdk.widget.AviaryGallery
		android:id="@+id/aviary_gallery"
		style="@style/AviaryWidget.AviaryGallery"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_above="@+id/aviary_bottom_line"
		android:layout_marginTop="@dimen/aviary_gallery_margin_top"
		android:clipChildren="true"
		android:clipToPadding="true" />

	<com.aviary.android.feather.sdk.widget.AviaryGalleryTopIndicatorView style="@style/AviaryWidget.BottomBar.GalleryTopIndicatorView" />

	<LinearLayout
		android:id="@+id/aviary_bottom_line"
		style="@style/AviaryGraphics.GalleryBottomLine"
		android:layout_alignParentBottom="true"
		android:orientation="vertical"></LinearLayout>

	<LinearLayout
		android:id="@+id/aviary_disable_status"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/aviary_panel_disabled_status"
		android:visibility="invisible"
		android:orientation="vertical" />

</RelativeLayout><!-- From: file:/Users/crugnola/Documents/git/aviary/Android-SDK/aviary-sdk/src/main/res/layout/aviary_panel_spot.xml --><!-- From: file:/D:/work/Feb_2015/CreativeSDKRoot/creativesdk/CreativeSDKImage/src/main/res/layout/aviary_panel_spot.xml -->