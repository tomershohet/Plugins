<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/AviaryDialog.MainLayout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center_horizontal"
	android:orientation="vertical"
	android:focusable="true">
    
    <requestFocus />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/aviary_logo_big" />

    <com.aviary.android.feather.sdk.widget.AviaryTextView
        android:id="@+id/aviary_version"
        style="?attr/aviaryDefaultTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/feather_version" />

    <LinearLayout style="@style/AviaryDialog.Divider" android:layout_marginBottom="10dip" android:layout_marginTop="5dip" />

    <com.aviary.android.feather.sdk.widget.AviaryTextView
        style="@style/AviaryDialog.Feedback.Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:id="@+id/aviary_text"
        android:text="@string/feather_feedback_dialog_message"
         />

    <com.aviary.android.feather.sdk.widget.AviaryButton
        android:id="@+id/aviary_button1"
        style="?attr/aviaryPrimaryButtonStyle"
        android:layout_marginBottom="5dip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:focusable="true"
        android:text="@string/feather_send_feedback" />
    
    <LinearLayout style="@style/AviaryDialog.Divider" android:layout_marginTop="0dip" android:layout_marginBottom="5dip" />

    <com.aviary.android.feather.sdk.widget.AviaryButton
        android:id="@+id/aviary_button2"
        style="?attr/aviarySecondaryButtonStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:focusable="true"
        android:text="@string/feather_close" />

</LinearLayout><!-- From: file:/Users/crugnola/Documents/git/aviary/Android-SDK/aviary-sdk/src/main/res/layout/aviary_feedback_dialog_view.xml --><!-- From: file:/D:/work/Feb_2015/CreativeSDKRoot/creativesdk/CreativeSDKImage/src/main/res/layout/aviary_feedback_dialog_view.xml -->