<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/AviaryWidget.BottomBar.Tool.FeedBack"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:focusable="false"
    android:focusableInTouchMode="false" >

    <LinearLayout
        android:layout_width="@dimen/aviary_nav_divider_double"
        android:layout_height="match_parent"
        android:layout_marginLeft="15dip"
        android:layout_marginRight="0dip"
        android:background="@drawable/aviary_tools_divider" >
    </LinearLayout>

    <com.aviary.android.feather.sdk.widget.AviaryHighlightImageButton
        android:focusable="false"
        android:id="@+id/image"
        style="@style/AviaryWidget.HighlightImageView.Tool"
        android:layout_marginLeft="@dimen/aviary_tool_margin"
        android:layout_marginRight="@dimen/aviary_tool_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/aviary_tool_ic_feedback" />

    <com.aviary.android.feather.sdk.widget.AviaryTextView
        android:focusable="false"
        android:id="@+id/text"
        style="?attr/aviaryBottomBarToolTextStyle"
        android:textAppearance="?attr/aviaryTextAppearance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dip"
        android:text="@string/feather_feedback" />

</LinearLayout><!-- From: file:/Users/crugnola/Documents/git/aviary/Android-SDK/aviary-sdk/src/main/res/layout/aviary_tool_feedback_layout.xml --><!-- From: file:/D:/work/Feb_2015/CreativeSDKRoot/creativesdk/CreativeSDKImage/src/main/res/layout/aviary_tool_feedback_layout.xml -->